{
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "Config",
            "Folder/with/File.php",
            "/\\.interface\\.php/",
            "{Infrastructure/.*}"
        ]
    },
    "timeout": 10,
    "logs": {
        "text": "infection.log",
        "html": "infection.html",
        "summary": "summary.log",
        "json": "infection-log.json",
        "perMutator": "per-mutator.md",
        "github": true,
        "badge": {
            "branch": "/^release-.*$/"
        }
    },
    "tmpDir": "/temp/infection",
    "mutators": {
        "@default": true
    },
    "testFramework":"codeception",
    "initialTestsPhpOptions": "-d zend_extension=xdebug.so"
}